{
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["airbnb", "prettier", "prettier/react"],
  "globals": {},
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "plugins": ["prettier", "jsx-a11y", "react", "import"],
  "rules": {
    "prettier/prettier": "error",
    "class-methods-use-this": 0,
    "react/prefer-stateless-function": 0
  }
}
